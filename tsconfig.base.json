{
  "compilerOptions": {
    "target": "esnext",
    "module": "CommonJS",
    "lib": ["esnext", "es2021"],
    "declaration": true,
    "declarationMap": true,
    "moduleResolution": "node",
    "esModuleInterop": true,
    "isolatedModules": true,
    "skipLibCheck": true,
    "resolveJsonModule": true
  }
}
